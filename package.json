{
  "name": "electron-react-starter",
  "version": "1.0.0",
  "description": "Electron React Starter",
  "main": "main.js",
  "scripts": {
    "start": "electron-forge start",
    "package": "electron-forge package",
    "make": "electron-forge make"
  },
  "keywords": [],
